void main(){
  int count = 1;
  String password = 'dfgsgsdfgdfgA1@';
  int minilength = 8;
  final RegExp hasUpparCase = RegExp(r'[A-Z]');
  final RegExp hasLowerCase = RegExp(r'[a-z]');
  final RegExp hasNumber = RegExp(r'[0-9]');
  final RegExp hasEspeacailChar = RegExp(r'[!@#$%^&*()]');
  
  if(password.length<minilength){
    print('Your password must should have 8 charecheter long');
  }
  else{
    if(hasUpparCase.hasMatch(password)){
      count++;
    }
    if(hasLowerCase.hasMatch(password)){
      count++;
    }
    if(hasNumber.hasMatch(password)){
      count++;
    }
    if(hasEspeacailChar.hasMatch(password)){
      count++;
    }
  }
  
  if(count==2){
    print('Your Password is Weak');
  }
  if(count==3){
    print('Your Password is Normal');
  }
  if(count==4){
    print('Your Password is Strong');
  }
  if(count==5){
    print('Your Password is very Strong');
  }
  
}
